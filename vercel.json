{
  "version": 2,
  "builds": [
    {
      "src": "proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "proxy.js"
    }
  ],
  "functions": {
    "api/proxy.js": {
      "maxDuration": 60,  // Set the max duration for the function
      "memory": 1024        // Allocate memory (optional)
    }
  }
}
