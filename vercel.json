{
  "version": 2,
  "functions": {
    "api/your-function.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/another-function.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/your-function",
      "dest": "/api/your-function.js"
    },
    {
      "src": "/api/another-function",
      "dest": "/api/another-function.js"
    }
  ]
}
